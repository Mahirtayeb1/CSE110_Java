import java.util.Scanner;  // printing digits left to r8 of an integer
public class pr2{  
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    //int b = sc.nextInt();
    //int c = sc.nextInt();
    //int prod = a*b*c;
    int count = 0;
    while(a!=0){
      System.out.print(a%10)
      
      a/=10;
    }
    //System.out.print(a*b*c);
  }
}