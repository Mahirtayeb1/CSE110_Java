// task 1
import java.util.Scanner;
public class task1{
  public static void main(String [] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int count = 0;
    for(int i = 2; count < n;i++){
      //System.out.print(i);
      //int c = 0;
      for(int j = 2; j<=i/2; j++){
        if(i==2){
          System.out.println(i);
          count ++;
        }
        else if(i%j!=0){
          System.out.println(i);
          count ++;
        }
      }
      //count ++;
    }
  }
}
