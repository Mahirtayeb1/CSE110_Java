import java.util.Scanner;
public class task5{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    for(int i = 1; i<=n; i++ ){
      int g = sc.nxtInt();
      if(g>=0){
        System.out.print(i + "is a Non-negative Integer");
      }
      else{
        System.out.print(i + "is a negative Integer");
      }
    }
  }
}