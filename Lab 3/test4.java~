import java.util.Scanner;
public class test4{
  public static void main(String [] args){
    Scanner sc = new Scanner (System.in);
    int n1 = sc.nextInt();
    //string oper = sc.next();
    //int n3 = sc.nextInt();
    if (n1 % 5==0 && n1 % 7==0){
      System.out.println("div by both");
    
    }
    else{
      if (n1%5==0){
        System.out.println("div by only 5");
      }
      else if(n1%7==0){
        System.out.println("div by 7");
      }
      else{
        System.out.println("No");
      }
    }
    
  }
    
}