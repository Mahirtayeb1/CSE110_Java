// task 13
public class task13{
  public static void main (String[] args){
   
    float dc = 1086;   // meter and 1 mile = 1609 meters
    float t = ((5) + (56 / 60) + (23 / (60*60))); // total time in hours
    float k = ((dc / t) / 1000);
    float m = ((dc / t) / 1609);
    
    System.out.println("Your velocity in km/h is " + k);
    System.out.println("Your velocity in miles/h is " + m);
    
  }
}